package com.Innspark.spring.boot.angularlogin.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@AllArgsConstructor
@NoArgsConstructor
@Data
@Getter
@Setter
@Entity

//previlege has properties id, name, createdate(create of this previlege date and time)
public class Previlege {
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private int id;
	private String name;
	private String createDate;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCreateDate() {
		return createDate;
	}
	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}
	public Previlege(int id, String name, String createDate) {
		super();
		this.id = id;
		this.name = name;
		this.createDate = createDate;
	}
	public Previlege() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "Previlege [id=" + id + ", name=" + name + ", createDate=" + createDate + "]";
	}
	
	
	

}
